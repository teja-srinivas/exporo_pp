{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "type": "project",
    "repositories": [
        {
            "type": "composer",
            "url": "https://nova.laravel.com"
        }
    ],
    "require": {
        "php": "^7.1.3",
        "ext-bcmath": "*",
        "ext-json": "*",
        "awesome-nova/filter-card": "^1.0",
        "cybercog/laravel-optimus": "^3.0",
        "dannyvankooten/laravel-vat": "dev-master",
        "docraptor/docraptor": "^1.3",
        "doctrine/dbal": "^2.7",
        "fideloper/proxy": "^4.0",
        "funkjedi/composer-include-files": "^1.0",
        "giggsey/libphonenumber-for-php": "^8.10",
        "guzzlehttp/guzzle": "^6.3",
        "intervention/validation": "^1.2",
        "itsgoingd/clockwork": "^3.0",
        "laravel/framework": "5.8.*",
        "laravel/nova": "^2.0",
        "laravel/tinker": "^1.0",
        "league/flysystem-aws-s3-v3": "^1.0",
        "maatwebsite/excel": "^3.1.8",
        "mormat/php-formula-interpreter": "^1.0",
        "overtrue/laravel-lang": "~3.0",
        "owen-it/laravel-auditing": "^8.0",
        "pragmarx/nova-boolean-datetime-field": "^1.0",
        "predis/predis": "^1.1",
        "psy/psysh": "^0.9.9",
        "s-ichikawa/laravel-sendgrid-driver": "^2.0",
        "sentry/sentry-laravel": "^0.11.0",
        "spatie/laravel-permission": "^2.12",
        "vyuldashev/nova-permission": "^1.4"
    },
    "require-dev": {
        "beyondcode/laravel-dump-server": "^1.0",
        "drfraker/snipe-migrations": "^1.1",
        "filp/whoops": "^2.0",
        "fzaninotto/faker": "^1.4",
        "mockery/mockery": "^1.0",
        "nunomaduro/collision": "^3.0",
        "nunomaduro/larastan": "^0.3.15",
        "phpunit/phpunit": "^7.5"
    },
    "autoload": {
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "include_files": [
            "app/helpers.php"
        ],
        "laravel": {
            "dont-discover": []
        }
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover"
        ],
        "prod": [
            "@composer install -o --no-progress --no-dev"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}