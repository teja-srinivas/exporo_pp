{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "type": "project",
    "repositories": [
        {
            "type": "composer",
            "url": "https://nova.laravel.com"
        },
        {
            "type": "vcs",
            "url": "https://github.com/felixgoldstein/laravel-vat.git"
        }
    ],
    "require": {
        "php": "^7.2",
        "ext-bcmath": "*",
        "ext-json": "*",
        "awesome-nova/filter-card": "^1.0",
        "calebporzio/parental": "0.9",
        "cybercog/laravel-optimus": "^3.0",
        "docraptor/docraptor": "^1.3",
        "doctrine/dbal": "^2.9.2 < 2.10",
        "fideloper/proxy": "^4.0",
        "funkjedi/composer-include-files": "^1.1.0",
        "giggsey/libphonenumber-for-php": "^8.10",
        "guzzlehttp/guzzle": "^6.3",
        "ibericode/vat": "^2.0",
        "intervention/validation": "^1.2",
        "itsgoingd/clockwork": "^4.0",
        "jenssegers/agent": "^2.6",
        "laravel/framework": "^6.3",
        "laravel/nova": "^2.1.1",
        "laravel/tinker": "^1.0",
        "league/flysystem-aws-s3-v3": "^1.0",
        "maatwebsite/excel": "^3.1.8",
        "mormat/php-formula-interpreter": "^1.0",
        "overtrue/laravel-lang": "~3.0",
        "owen-it/laravel-auditing": "^9.3.1",
        "pragmarx/nova-boolean-datetime-field": "^1.0",
        "predis/predis": "^1.1",
        "psy/psysh": "^0.9.9",
        "s-ichikawa/laravel-sendgrid-driver": "^2.0",
        "sentry/sentry-laravel": "^1.1.0",
        "spatie/laravel-permission": "^3.0"
    },
    "require-dev": {
        "drfraker/snipe-migrations": "^1.1",
        "escapestudios/symfony2-coding-standard": "^3.9",
        "facade/ignition": "^1.4",
        "fzaninotto/faker": "^1.4",
        "johnkary/phpunit-speedtrap": "^3.1",
        "mockery/mockery": "^1.0",
        "nunomaduro/collision": "^3.0",
        "nunomaduro/larastan": "^0.4",
        "phpunit/phpunit": "^8.0",
        "slevomat/coding-standard": "^5.0"
    },
    "autoload": {
        "classmap": [
            "database/seeds",
            "database/migrations",
            "database/factories"
        ],
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "include_files": [
            "app/helpers.php"
        ],
        "laravel": {
            "dont-discover": []
        }
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover"
        ],
        "prod": [
            "@composer install -o --no-progress --no-dev"
        ],
        "fix:style": "phpcbf",
        "lint:style": "phpcs",
        "lint": [
            "@lint:style",
            "php artisan code:analyse"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
