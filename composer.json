{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "type": "project",
    "repositories": [
        {
            "type": "composer",
            "url": "https://nova.laravel.com"
        },
        {
            "type": "vcs",
            "url": "https://github.com/felixgoldstein/laravel-vat.git"
        }
    ],
    "require": {
        "php": "^8.0",
        "ext-bcmath": "*",
        "ext-json": "*",
        "awesome-nova/filter-card": "^1.0",
        "calebporzio/parental": "1.3.1",
        "cybercog/laravel-optimus": "^3.8.0",
        "docraptor/docraptor": "^3.0.1",
        "doctrine/dbal": "^2.13.9",
        "fideloper/proxy": "^4.4.2",
        "funkjedi/composer-include-files": "^1.0",
        "giggsey/libphonenumber-for-php": "^8.12",
        "guratr/nova-command-runner": "^1.0",
        "guzzlehttp/guzzle": "^7.4",
        "ibericode/vat": "^2.0.6",
        "illuminate/support": "9.21.6",
        "intervention/validation": "^3.2.0",
        "itsgoingd/clockwork": "^5.1.6",
        "jenssegers/agent": "^2.6",
        "laravel/framework": "^9.21.6",
        "laravel/nova": "^2.1.1",
        "laravel/tinker": "^2.7",
        "league/flysystem-aws-s3-v3": "^3.0.2",
        "maatwebsite/excel": "^3.1.8",
        "mormat/php-formula-interpreter": "^1.0",
        "overtrue/laravel-lang": "^6.0.3",
        "owen-it/laravel-auditing": "^13.0.4",
        "pragmarx/nova-boolean-datetime-field": "^1.0",
        "predis/predis": "^2.0",
        "psy/psysh": "^0.11.7",
        "s-ichikawa/laravel-sendgrid-driver": "^4.0.1",
        "sentry/sentry-laravel": "^2.13.0",
        "spatie/laravel-permission": "^5.5.5"
    },
    "require-dev": {
        "drfraker/snipe-migrations": "^1.1",
        "escapestudios/symfony2-coding-standard": "^3.9",
        "facade/ignition": "^2.17.6",
        "fzaninotto/faker": "^1.4",
        "johnkary/phpunit-speedtrap": "^3.1",
        "mockery/mockery": "^1.0",
        "nunomaduro/collision": "^3.0",
        "nunomaduro/larastan": "^2.1.12",
        "phpunit/phpunit": "^8.0",
        "slevomat/coding-standard": "^8.3.0"
    },
    "autoload": {
        "classmap": [
            "database/seeds",
            "database/migrations",
            "database/factories"
        ],
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "include_files": [
            "app/helpers.php"
        ],
        "laravel": {
            "dont-discover": []
        }
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover"
        ],
        "prod": [
            "@composer install -o --no-progress --no-dev"
        ],
        "fix:style": "phpcbf",
        "lint:style": "phpcs",
        "lint": [
            "@lint:style"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true,
        "github-oauth": {
            "github.com": "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"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
